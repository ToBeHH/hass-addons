{
  "name": "LCN-PCHK",
  "version": "1.0.0",
  "slug": "lcnpchk",
  "description": "Runs LCN-PCHK",
  "arch": ["amd64", "armhf", "armv7", "aarch64"],
  "url": "https://github.com/alengwenus/hass-addons/tree/main/lcn-pchk",
  "startup": "services",
  "boot": "auto",
  "ports": {
    "4114/tcp": 4114,
    "4220/tcp": 4220
  },
  "ports_description": {
    "4114/tcp": "PCHK port",
    "4220/tcp": "RPC port"
  },
  "uart": true,
  "usb": true,
  "apparmor": true,
  "options": {
    "device": "ttyUSB0",
    "user": "lcn",
    "password": "lcn",
    "licensee": "lcnpchk",
    "license_keys": []
  },
  "schema": {
    "device": "str",
    "user": "str",
    "password": "str",
    "licensee": "str",
    "license_keys": ["str"],
    "host_id": "int?",
    "sync_time": "list(auto|system|disabled|pool.ntp.org|time.nist.gov|time-nw.nist.gov|time-a.nist.gov|time-b.nist.gov|ptbtime1.ptb.de|ptbtime2.ptb.de)?",
    "timeout": "int?"
  }
}
